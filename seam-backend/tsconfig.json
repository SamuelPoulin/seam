{
  "compilerOptions": {
    "esModuleInterop": true,
    "target": "es5",
    "moduleResolution": "node",
    "module": "commonjs",
    "lib": [
      "es6"
    ],
    "strict": true,
    "types": [
      "reflect-metadata"
    ],
    "forceConsistentCasingInFileNames": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "sourceMap": false,
    "outDir": "out",
  }
}